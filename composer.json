{
    "name": "davedevelopment/pimple-aware-event-dispatcher",
    "description": "A Symfony compatible event dispatcher that allows services to be lazy loaded from an instance of the Pimple DI container",
    "authors": [
        {
            "name": "Dave Marshall",
            "email": "dave.marshall@atstsolutions.co.uk"
        }
    ],
    "require": {
        "pimple/pimple": "3.*",
        "symfony/event-dispatcher": "~2.1|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": {
            "PimpleAwareEventDispatcher": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
